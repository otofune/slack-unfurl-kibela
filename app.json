{
  "name": "Syakusi",
  "description": "The slack app to unfurl kibela links",
  "repository": "https://github.com/otofune/slack-unfurl-kibela",
  "env": {
    "SYAKUSI_SLACK_BOT_USER_TOKEN": {
      "description": "Kibela team name"
    },
    "SYAKUSI_KIBELA_TEAM_NAME": {
      "description": "Get from https://my.kibe.la/settings/access_tokens"
    },
    "SYAKUSI_KIBELA_ACCESS_TOKEN": {
      "description": "Slack app's OAuth token"
    }
  }
}